$(document).ready(function() {
  $(if('#blueprint').length){
    //variables
    var scrollHeight,
        scrolled = false,
        containter = $('#blueprint'),
        itemsDisplayed = container.find('img'),
        windowHeight = $(window).height(),
        windowWidth = $(window).width(),
        marginHeight = windowHeight - 100;
    
  // NEW //
  var distanceMultiplier = 1.75
  itemsDisplayed.each(initSlices(i, distanceMultiplier)  
  
  //Sets the attributes for each slice of the final image based on 
  function initSlices(sliceNumber, distanceMultiplier) {
    var imgSlice = itemsDisplayed($this)
    var xOffset = GETMEFROMDAJSON(i)
    var leftRight = GETMEFROMDAJSONTOO(i)
    imgSlice.data('params', {
      x0: -(windowWidth * distanceMultiplier),
      x1: $(this).css(leftRight),
      xOff: xOffset
    });
    if (i < totalSlices/2) {
      distanceMultiplier -= .015
    } else {
      distanceMultiplier += .015
    }

  });

  }

  function scrollTo (x0, x1, scrollTop) {
    var s_max = scrollHeight / 0.5;
    return Math.min((-p0 + p1) / s_max * scrollTop + p0, p1);
  }

  //initializes the images on the outer bounds of the window
  function init() {
  }
});
